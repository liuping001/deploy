deploy_info:
  server1:
    push:
      - src: /tmp/1.txt
        dest: /tmp/1_1.txt
    start: "cd / && cat /tmp/1_1.txt"
    stop: "cd / && rm /tmp/1_1.txt"
    restart: "cd / && ls -l"

  server2:
    push:
      - src: /tmp/2.txt
        dest: /tmp/2_1.txt  
    start: "cd / && cat /tmp/1_1.txt"
    stop: "cd / && rm /tmp/2_1.txt"
    restart: "cd / && ls -l"
